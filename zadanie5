#include <iostream>
#include <stdlib.h>

using namespace std;


namespace kartoteka
{
class Osoba
{
public:
    string a,b;
    Osoba()
    {
        a="Gall";
        b="Anonim";
    }
    Osoba(string imie,string nazwisko)
    {
        a=imie;
        b=nazwisko;
    }
    string getimie()
    {
        return a;
    };
    string getnazwisko()
    {
        return b;
    };
};


namespace mockup
{
class Kartoteka
{
